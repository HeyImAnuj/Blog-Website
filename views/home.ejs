<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= homeContent %></p>
<% newPost.forEach(function(element){ %>
    <h3><%= element.title %></h3>
    <p>
        <%= element.body.slice(0, 100)+" ..." %> 
        <a href="/posts/<%=element.title%>" >
            Read More
        </a>
    </p>
    
<% }); %>
<%- include('partials/footer') -%>